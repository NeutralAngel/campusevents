<table class="large-sidebar">
  <thead>
    <tr>
      <th>Name</th>
      <th>Start Date</th>
      <th>End Date</th>
    </tr>
  </thead>
  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= link_to event.name, event %></td>
        <td><%= event.start_date.strftime('%m-%d-%Y') %></td>
        <td><%= event.end_date.strftime('%m-%d-%Y') %></td>
      </tr>
    <% end %>
  </tbody>
</table>